<?xml version="1.0" encoding="utf-8"?>
<!--
    Backup rules for Android Auto Backup (API 23-30)
    See: https://developer.android.com/guide/topics/data/autobackup
    Note: This file is ignored for devices running API 31+; use data_extraction_rules.xml instead.
-->
<full-backup-content>
    <!-- Include all shared preferences -->
    <include domain="sharedpref" path="." />

    <!-- Exclude connection settings (hostname/port) for security -->
    <exclude domain="sharedpref" path="ComfyChairPrefs.xml" />
</full-backup-content>

{
  "name": "Wan 2.2 I2V (LightX2V)",
  "description": "Default workflow for Wan 2.2 image to video generation (LightX2V LoRA)",
  "defaults": {
    "width": 480,
    "height": 800,
    "length": 33,
    "frame_rate": 16
  },
  "nodes": {
    "1": {
      "class_type": "CLIPLoader",
      "inputs": {
        "clip_name": "{{clip_name}}",
        "type": "wan",
        "device": "default"
      },
      "_meta": {
        "title": "Load CLIP"
      }
    },
    "2": {
      "class_type": "VAELoader",
      "inputs": {
        "vae_name": "{{vae_name}}"
      },
      "_meta": {
        "title": "Load VAE"
      }
    },
    "3": {
      "class_type": "UNETLoader",
      "inputs": {
        "unet_name": "{{highnoise_unet_name}}",
        "weight_dtype": "default"
      },
      "_meta": {
        "title": "Load Diffusion Model (High Noise)"
      }
    },
    "4": {
      "class_type": "UNETLoader",
      "inputs": {
        "unet_name": "{{lownoise_unet_name}}",
        "weight_dtype": "default"
      },
      "_meta": {
        "title": "Load Diffusion Model (Low Noise)"
      }
    },
    "5": {
      "class_type": "LoadImage",
      "inputs": {
        "image": "{{image_filename}}"
      },
      "_meta": {
        "title": "Load Image"
      }
    },
    "6": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "{{positive_prompt}}",
        "clip": ["1", 0]
      },
      "_meta": {
        "title": "CLIP Text Encode (Positive Prompt)"
      }
    },
    "7": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "色调艳丽，过曝，静态，细节模糊不清，字幕，风格，作品，画作，画面，静止，整体发灰，最差质量，低质量，JPEG压缩残留，丑陋的，残缺的，多余的手指，画得不好的手部，画得不好的脸部，畸形的，毁容的，形态畸形的肢体，手指融合，静止不动的画面，杂乱的背景，三条腿，背景人很多，倒着走",
        "clip": ["1", 0]
      },
      "_meta": {
        "title": "CLIP Text Encode (Negative Prompt)"
      }
    },
    "8": {
      "class_type": "WanImageToVideo",
      "inputs": {
        "width": "{{width}}",
        "height": "{{height}}",
        "length": "{{length}}",
        "batch_size": 1,
        "positive": ["6", 0],
        "negative": ["7", 0],
        "vae": ["2", 0],
        "start_image": ["5", 0]
      },
      "_meta": {
        "title": "WanImageToVideo"
      }
    },
    "9": {
      "class_type": "LoraLoaderModelOnly",
      "inputs": {
        "lora_name": "{{highnoise_lora_name}}",
        "strength_model": 1.0000000000000002,
        "model": ["3", 0]
      },
      "_meta": {
        "title": "LoraLoaderModelOnly (High Noise)"
      }
    },
    "10": {
      "class_type": "LoraLoaderModelOnly",
      "inputs": {
        "lora_name": "{{lownoise_lora_name}}",
        "strength_model": 1.0000000000000002,
        "model": ["4", 0]
      },
      "_meta": {
        "title": "LoraLoaderModelOnly (Low Noise)"
      }
    },
    "11": {
      "class_type": "ModelSamplingSD3",
      "inputs": {
        "shift": 5.000000000000001,
        "model": ["9", 0]
      },
      "_meta": {
        "title": "ModelSamplingSD3 (High Noise)"
      }
    },
    "12": {
      "class_type": "ModelSamplingSD3",
      "inputs": {
        "shift": 5.000000000000001,
        "model": ["10", 0]
      },
      "_meta": {
        "title": "ModelSamplingSD3 (Low Noise)"
      }
    },
    "13": {
      "class_type": "KSamplerAdvanced",
      "inputs": {
        "add_noise": "enable",
        "noise_seed": 42,
        "steps": 4,
        "cfg": 1,
        "sampler_name": "euler",
        "scheduler": "simple",
        "start_at_step": 0,
        "end_at_step": 2,
        "return_with_leftover_noise": "enable",
        "model": ["11", 0],
        "positive": ["8", 0],
        "negative": ["8", 1],
        "latent_image": ["8", 2]
      },
      "_meta": {
        "title": "KSampler (Advanced) - High Noise"
      }
    },
    "14": {
      "class_type": "KSamplerAdvanced",
      "inputs": {
        "add_noise": "disable",
        "noise_seed": 0,
        "steps": 4,
        "cfg": 1,
        "sampler_name": "euler",
        "scheduler": "simple",
        "start_at_step": 2,
        "end_at_step": 4,
        "return_with_leftover_noise": "disable",
        "model": ["12", 0],
        "positive": ["8", 0],
        "negative": ["8", 1],
        "latent_image": ["13", 0]
      },
      "_meta": {
        "title": "KSampler (Advanced) - Low Noise"
      }
    },
    "15": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["14", 0],
        "vae": ["2", 0]
      },
      "_meta": {
        "title": "VAE Decode"
      }
    },
    "16": {
      "class_type": "CreateVideo",
      "inputs": {
        "fps": "{{frame_rate}}",
        "images": ["15", 0]
      },
      "_meta": {
        "title": "Create Video"
      }
    },
    "17": {
      "class_type": "SaveVideo",
      "inputs": {
        "filename_prefix": "video/ComfyChair",
        "format": "auto",
        "codec": "auto",
        "video": ["16", 0]
      },
      "_meta": {
        "title": "Save Video"
      }
    }
  }
}

{
  "name": "Flux",
  "description": "Workflow for Flux UNET model with dual CLIP",
  "defaults": {
    "width": 1024,
    "height": 1024,
    "steps": 30,
    "sampler_name": "euler",
    "scheduler": "simple",
    "has_negative_prompt": false,
    "has_cfg": false,
    "has_clip_name": false,
    "has_clip_name1": true,
    "has_clip_name2": true,
    "has_unet_name": true
  },
  "nodes": {
    "1": {
      "class_type": "UNETLoader",
      "inputs": {
        "unet_name": "{{unet_name}}",
        "weight_dtype": "default"
      },
      "_meta": {
        "title": "Load Diffusion Model"
      }
    },
    "2": {
      "class_type": "VAELoader",
      "inputs": {
        "vae_name": "{{vae_name}}"
      },
      "_meta": {
        "title": "Load VAE"
      }
    },
    "3": {
      "class_type": "DualCLIPLoader",
      "inputs": {
        "clip_name1": "{{clip_name1}}",
        "clip_name2": "{{clip_name2}}",
        "type": "flux",
        "device": "default"
      },
      "_meta": {
        "title": "DualCLIPLoader"
      }
    },
    "4": {
      "class_type": "EmptyLatentImage",
      "inputs": {
        "width": "{{width}}",
        "height": "{{height}}",
        "batch_size": 1
      },
      "_meta": {
        "title": "Empty Latent Image"
      }
    },
    "5": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "{{positive_prompt}}",
        "clip": ["3", 0]
      },
      "_meta": {
        "title": "CLIP Text Encode (Prompt)"
      }
    },
    "6": {
      "class_type": "RandomNoise",
      "inputs": {
        "noise_seed": 0
      },
      "_meta": {
        "title": "RandomNoise"
      }
    },
    "7": {
      "class_type": "KSamplerSelect",
      "inputs": {
        "sampler_name": "{{sampler_name}}"
      },
      "_meta": {
        "title": "KSamplerSelect"
      }
    },
    "8": {
      "class_type": "BasicScheduler",
      "inputs": {
        "scheduler": "{{scheduler}}",
        "steps": "{{steps}}",
        "denoise": 1.0,
        "model": ["1", 0]
      },
      "_meta": {
        "title": "BasicScheduler"
      }
    },
    "9": {
      "class_type": "BasicGuider",
      "inputs": {
        "model": ["1", 0],
        "conditioning": ["5", 0]
      },
      "_meta": {
        "title": "BasicGuider"
      }
    },
    "10": {
      "class_type": "SamplerCustomAdvanced",
      "inputs": {
        "noise": ["6", 0],
        "guider": ["9", 0],
        "sampler": ["7", 0],
        "sigmas": ["8", 0],
        "latent_image": ["4", 0]
      },
      "_meta": {
        "title": "SamplerCustomAdvanced"
      }
    },
    "11": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["10", 0],
        "vae": ["2", 0]
      },
      "_meta": {
        "title": "VAE Decode"
      }
    },
    "12": {
      "class_type": "SaveImage",
      "inputs": {
        "filename_prefix": "ComfyChair",
        "images": ["11", 0]
      },
      "_meta": {
        "title": "Save Image"
      }
    }
  }
}

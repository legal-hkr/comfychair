{
  "name": "SDXL LCM with latent upscaling",
  "description": "Workflow for SDXL LCM generation with latent upscaling",
  "defaults": {
    "width": 832,
    "height": 1216,
    "steps": 30,
    "cfg": 2.0,
    "sampler_name": "lcm",
    "scheduler": "beta",
    "negative_prompt": ""
  },
  "nodes": {
    "1": {
      "class_type": "CheckpointLoaderSimple",
      "inputs": {
        "ckpt_name": "{{ckpt_name}}"
      },
      "_meta": {
        "title": "Load Checkpoint"
      }
    },
    "2": {
      "class_type": "ModelSamplingDiscrete",
      "inputs": {
        "sampling": "lcm",
        "zsnr": false,
        "model": ["1", 0]
      },
      "_meta": {
        "title": "ModelSamplingDiscrete"
      }
    },
    "3": {
      "class_type": "CLIPSetLastLayer",
      "inputs": {
        "stop_at_clip_layer": -2,
        "clip": ["1", 1]
      },
      "_meta": {
        "title": "CLIP Set Last Layer"
      }
    },
    "4": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "{{positive_prompt}}",
        "clip": ["3", 0]
      },
      "_meta": {
        "title": "CLIP Text Encode (Positive Prompt)"
      }
    },
    "5": {
      "class_type": "CLIPTextEncode",
      "inputs": {
        "text": "{{negative_prompt}}",
        "clip": ["3", 0]
      },
      "_meta": {
        "title": "CLIP Text Encode (Negative Prompt)"
      }
    },
    "6": {
      "class_type": "EmptyLatentImage",
      "inputs": {
        "width": "{{width}}",
        "height": "{{height}}",
        "batch_size": 1
      },
      "_meta": {
        "title": "Empty Latent Image"
      }
    },
    "7": {
      "class_type": "KSampler",
      "inputs": {
        "seed": 0,
        "steps": "{{steps}}",
        "cfg": "{{cfg}}",
        "sampler_name": "{{sampler_name}}",
        "scheduler": "{{scheduler}}",
        "denoise": 1.0,
        "model": ["2", 0],
        "positive": ["4", 0],
        "negative": ["5", 0],
        "latent_image": ["6", 0]
      },
      "_meta": {
        "title": "KSampler"
      }
    },
    "8": {
      "class_type": "LatentUpscaleBy",
      "inputs": {
        "upscale_method": "nearest-exact",
        "scale_by": 1.5,
        "samples": ["7", 0]
      },
      "_meta": {
        "title": "Upscale Latent By"
      }
    },
    "9": {
      "class_type": "KSampler",
      "inputs": {
        "seed": 0,
        "steps": 30,
        "cfg": 2.0,
        "sampler_name": "lcm",
        "scheduler": "exponential",
        "denoise": 0.5,
        "model": ["2", 0],
        "positive": ["4", 0],
        "negative": ["5", 0],
        "latent_image": ["8", 0]
      },
      "_meta": {
        "title": "KSampler (Refine)"
      }
    },
    "10": {
      "class_type": "VAEDecode",
      "inputs": {
        "samples": ["9", 0],
        "vae": ["1", 2]
      },
      "_meta": {
        "title": "VAE Decode"
      }
    },
    "11": {
      "class_type": "SaveImage",
      "inputs": {
        "filename_prefix": "ComfyChair",
        "images": ["10", 0]
      },
      "_meta": {
        "title": "Save Image"
      }
    }
  }
}
